{
  "name": "add-asset-html-webpack-plugin",
  "version": "1.0.2",
  "description": "Add a JS or CSS assets to a generated HTML file",
  "files": [
    "lib/",
    "index.d.ts"
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "del lib/ coverage/ .nyc_output/ example/build example/dist",
    "compile": "babel src/ --out-dir lib/",
    "cover": "jest --coverage",
    "preexample": "npm run clean && npm run compile",
    "example": "webpack --config example/webpack.config.dll.js && webpack --config example/webpack.config.js",
    "lint": "eslint .",
    "prepublish": "not-in-install && npm run compile || in-install",
    "postpublish": "git push --follow-tags",
    "pretest": "npm run lint",
    "test": "jest",
    "pretravis": "npm run lint && npm run clean",
    "travis": "npm run cover"
  },
  "repository": "SimenB/add-asset-html-webpack-plugin",
  "keywords": [
    "dll",
    "html-webpack-plugin",
    "webpack"
  ],
  "author": "Simen Bekkhus <sbekkhus91@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SimenB/add-asset-html-webpack-plugin/issues"
  },
  "homepage": "https://github.com/SimenB/add-asset-html-webpack-plugin#readme",
  "dependencies": {
    "bluebird": "^3.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-module-method": "^6.16.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-register": "^6.18.0",
    "classnames": "^2.2.5",
    "del-cli": "^0.2.0",
    "eslint": "^3.9.1",
    "eslint-config-simenb-base": "^8.0.0",
    "html-webpack-plugin": "^2.10.0",
    "in-publish": "^2.0.0",
    "jest": "^17.0.3",
    "nyc": "^10.0.0",
    "power-assert": "^1.4.2",
    "sinon": "^1.17.6",
    "webpack": "^1.13.3"
  },
  "peerDependencies": {
    "html-webpack-plugin": "^2.10.0"
  },
  "engines": {
    "node": ">=4"
  },
  "jest": {
    "testRegex": "test.js",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
